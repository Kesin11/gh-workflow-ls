    "compile:darwin-amd64": "deno compile --allow-env --allow-net --allow-read --target x86_64-apple-darwin -o ./dist/gh-workflow-ls-darwin-amd64 workflow-ls.ts",
    "compile:darwin-arm64": "deno compile --allow-env --allow-net --allow-read --target aarch64-apple-darwin -o ./dist/gh-workflow-ls-darwin-arm64 workflow-ls.ts"
  }
}
